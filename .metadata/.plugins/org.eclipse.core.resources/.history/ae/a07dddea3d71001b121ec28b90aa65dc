int count_e(char* tab){  
	/*A COMPLETER */
	int nb_e_c = 0;
	char ch;
	for(int i=0 ; i < strlen(string1) ; i++)
	{
		ch = string1[i];
		if(string1[i] == 'e')
			nb_e_c++;
	}
	asm("LDR r0,=nb_e_c\n\t"
		"LDR r0,[r0]\n\t"
	);
}

